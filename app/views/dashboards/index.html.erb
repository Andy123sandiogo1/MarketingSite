												    	<th class="font-fix month-name" scope="col">Nov</th>
												    	<th class="font-fix month-name" scope="col">Dec</th>
												    	<th class="font-fix month-name" scope="col">Total Return</th>
												    </tr>
												</thead>
												<tbody>
												    <tr>
												      	<th scope="row" class="year-name font-fix">2017</th>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="loss-return">-20.76%</td>
													    <td class="loss-return">-20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">+ 200.65%</td>
												    </tr>
												    <tr>
												      	<th scope="row" class="year-name font-fix">2018</th>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
													    <td class="profit-return">20.76%</td>
												    </tr>
												    <tr>
												    	<th colspan="13"></th>
												    	<td class="total-return">+ 200.65%</td>
												    </tr>
												</tbody>
											</table>
										</div> <!-- /.performance-monthly-table-sheet -->
									</div> <!-- /.tab-pane -->



									<div class="tab-pane fade" id="performance-weekly" role="tabpanel" aria-labelledby="performance-weekly-tab">
									  	<select class="theme-select-dropdown weekly-dropdown">
											<option value="Value 1">August 2018</option>
											<option value="Value 1">July 2018</option>
											<option value="Value 1">June 2018</option>
											<option value="Value 1">May 2018</option>
											<option value="Value 1">April 2018</option>
											<option value="Value 1">March 2018</option>
											<option value="Value 1">February 2018</option>
										</select>

										<ul class="weekly-performance-data">
											<li class="week-data clearfix">
												<div class="week-frame font-fix">August 13 - August 19 <span class="current-week">(Current Week)</span></div>
											</li>
											<li class="week-data clearfix">
												<div class="week-frame font-fix">August 6 - August 12</div>
												<div class="profit-return">10.05%</div>
											</li>
											<li class="week-data clearfix">
												<div class="week-frame font-fix">July 30 - August 5</div>
												<div class="profit-return">10.05%</div>
											</li>
										</ul>
									</div> <!-- /.tab-pane -->
								</div> <!-- /.tab-content -->
							</div> <!-- /.full-performance-history -->
								
							<div class="fund-information-table table-responsive">
								<table class="table">
									<tbody>
									    <tr>
										    <td>
										    	<div class="title">Manager</div>
										    	<div class="text font-fix">Coinmitted IBC</div>
										    </td>
